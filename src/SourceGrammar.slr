%module SampleGrammar 

%import Grammar

%tokens 
_         " |\n"
PlusT     "\+"
MultT     "\*"
VarT      "[a-z]+"
LBraceT   "\("
RBraceT   "\)"


%grammar
E -> E PlusT T          { Add $0 $2 }
   | T                  { $0 }

T -> T MultT F          { Mult $0 $2 }
   | F                  { $0 }

F -> LBraceT E RBraceT  { $1 }
   | VarT               { Var $0 }



